<h1>Listing questoes</h1>

<table>
  <tr>
    <th>Prova</th>
    <th>Pergunta</th>
    <th>Resposta</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questoes.each do |questao| %>
  <tr>
    <td><%= questao.prova_id %></td>
    <td><%= questao.pergunta %></td>
    <td><%= questao.resposta %></td>
    <td><%= link_to 'Show', questao %></td>
    <td><%= link_to 'Edit', edit_questao_path(questao) %></td>
    <td><%= link_to 'Destroy', questao, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Questao', new_questao_path %>
